import numpy as np
import time

numpy_array =np.arange(1, 50001)
python_list = list(range(1, 50001))

start_time = time.time()
numpy_sum = np.sum(numpy_array)
numpy_time = time.time() - start_time

start_time = time.time()
python_sum = sum(python_list)
python_time = time.time() - start_time

print(f"Numpy Sum: {numpy_sum}")
print(f"Python sum: {python_sum}")
print(f"Numpy Time: {numpy_time:.6f} seconds")
print(f"Python Time: {python_time:.6f} seconds")

speed_diff = python_time / numpy_time
print(f"Python is {speed_diff:.1f}x faster")

